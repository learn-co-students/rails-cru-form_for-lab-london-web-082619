<h1><%= @artist.name %></h1>
<p><%= @artist.bio %></p><br>
<table style="width:50%">
  <tr>
    <th>Song</th>
    <th>Genre</th>
  </tr>
  <% @artist.songs.each do |song| %>
  <tr>
    <td><%= link_to song.name, song_path(song) %></td>
    <td><%= link_to song.genre.name, genre_path(song.genre) %></td>
  </tr>
  <% end %>
  </table>

  <%= link_to 'Edit', edit_artist_path(@artist) %>