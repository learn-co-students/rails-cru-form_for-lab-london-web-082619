<h1><%= @genre.name %></h1>
<table style="width:50%">
  <tr>
    <th>Song</th>
    <th>Artist</th>
  </tr>
  <% @genre.songs.each do |song| %>
  <tr>
    <td><%= link_to song.name, song_path(song) %></td>
    <td><%= link_to song.artist.name, artist_path(song.artist) %></td>
  </tr>
  <% end %>
  </table>

  <%= link_to 'Edit', edit_genre_path %>