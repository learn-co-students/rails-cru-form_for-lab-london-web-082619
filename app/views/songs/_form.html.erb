<%= form_for @song do |f| %>
  <%= f.label :name %> <br>
  <%= f.text_field :name %> <br>
  <br>

  <%= f.label :artist_id, "Artist ID" %> <br>
  <%= f.number_field :artist_id %> <br>
  <br>

  <%= f.label :genre_id, "Genre ID" %> <br>
  <%= f.number_field :genre_id %> <br>
  <br>

  <%= f.submit %>

<% end %>
